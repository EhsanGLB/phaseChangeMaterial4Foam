// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

transportModel		PCM4Foam;
type			forcedConvection;

rho		rho [1 -3 0 0 0 0 0]		996;
Cp		Cp [0 2 -2 -1 0 0 0]		4200;
kappa		kappa [ 1 1 -3 -1 0 0 0 ]	0.6;
mu		mu [ 1 -1 -1 0 0 0 0 ]		9e-4;
beta            beta [0 0 0 -1 0 0 0]		2.1e-4;
Prt		Prt [0 0 0 0 0 0 0]		0.85;

