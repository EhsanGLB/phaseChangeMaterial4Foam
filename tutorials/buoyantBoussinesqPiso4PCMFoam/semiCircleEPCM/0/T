FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 317;

boundaryField
{
    innerWall
    {
        type            fixedValue;
        value           uniform 322;
    }

    outerWall
    {
        type            fixedValue;
        value           uniform 312;
    }

    horizontalWalls
    {
	type		zeroGradient;
    }

    frontAndBack
    {
	type		empty;
    }
}
