FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300;

boundaryField
{
    right
    {
        type            timeVaryingUniformFixedValue;
        fileName        "$FOAM_CASE/constant/tables/TempvsTime";
        outOfBounds     clamp;           // (error|warn|clamp|repeat)
    }

    ".*"
    {
	type		zeroGradient;
    }
}

